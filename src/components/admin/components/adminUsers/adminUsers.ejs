<div id="adminUsers" class="content">
	<h4 class="w3-container w3-padding-16">Utilisateurs</h4>
	<span class="error"><%= error %></span>
	<div>
		<ul id="users-list" class='w3-ul'>
			<% for(var i=0; i<users.length; i++) {%>
			<li class="w3-display-container">
				<p id='<%= users[i].id %>'>
					<a href='/#/admin/users/<%= users[i].id %>' class='w3-text-gray w3-hover-none w3-hover-text-black'><%= users[i].email %></a>
				</p>
			</li>
			<% } %>
		</ul>
	</div>
</div>
