<div id="adminBooks" class="content">
	<h4 class="w3-container w3-padding-16">Ouvrages</h4>
	<span class="error"><%= error %></span>
	<div>
		<ul id="books-list" class='w3-ul'>
			<% for(var i=0; i<books.length; i++) {%>
			<li class="w3-display-container">
				<p><%= books[i].author %></p>
				<p>
					<a href='/#/admin/books/<%= books[i].id %>' class='w3-text-gray w3-hover-none w3-hover-text-black'><%= books[i].title %></a>
				</p>
			</li>
			<% } %>
		</ul>
	</div>
</div>
